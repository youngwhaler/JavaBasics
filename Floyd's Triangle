import java.util.*;

public class JavaBasics{

public static void floyds_triangle(int n){
    int counter = 1;
    //outer
    for(int i = 1; i<=n; i++){
      //inner loop 
      for(int j = 1; j<=i; j++){
        System.out.print(counter + " ");
        counter++;
      }
      System.out.println();
    }
  }


  public static void main(String args[]){
    Scanner sc = new Scanner(System.in);
    System.out.print("enter number: ");
    int n = sc.nextInt();
    floyds_triangle(n);
    
  }
  
}
